          bot_retor+=" â’  ${firsnme} ${lastnme} â’\n"
		  [[ -z ${nUSER} ]] && bot_retor+=" âš ï¸ USUARIO SIN ALIAS SOLICITO $(cat < /root/RegBOT/U_check.txt | wc -l) VECES âš ï¸\n" || bot_retor+=" <u> UserName </u>: @${nUSER} SOLICITO $(($n_soli + 1)) VECES\n"
		  bot_retor+=" SOLICITUD NÂ° $(cat < /root/RegBOT/U_check.txt | wc -l) RECIVIDA A LAS $(date +%R) âœ…\n"
		  bot_retor+=" $(call.barras 'â•' '33') \n"
          bot_retor+=" VERIFIQUE SU INFORMACION ANTES DE APROBAR\n"
          bot_retor+=" â”€â”€â”€ Â· Â· Â· â”€â”€â”€â”€ ê’° ê’± â”€â”€â”€â”€ Â· Â· Â· â”€â”€â”€ \n"
          #bot_retor+=" $(call.barras 'â•' '33') \n"
		  call.ITEM.fijos "$(cat < ${CIDdir}/Admin-ID)" "${bot_retor}" "${id_solicitante}"
